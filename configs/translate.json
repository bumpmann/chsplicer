{
    "version": "1.1.0",
    "output": "{{1}}",
    "song": "{{1}}",
    "copy": true,
    "start_blank": 0,
    "ignoreAudio": true,
    "args": {
        "1": {"resolve": true},
        "method": {"default": "ai"}
    },
    "plugins": [
        ["aiTranslator", {"if": "{{eq method \"ai\"}}", "path": "translations/local", "overwrite": "{{overwrite}}"}],
        ["dictionaryTranslator", {"if": "{{eq method 'dictionary'}}", "chart": "translations/local", "overwrite": "{{overwrite}}"}]
    ],
    "parts": [{"start": 0, "event": true, "end": -1}]
}